<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <!-- 
    Les clés Services obligatoires doivent contenir : nom windows du service; titre pour affichage; serveur; contexte; tooltip 
    Il est possible de configurer des clés supplémentaires :
    - Tooltip_# : pour affichage d'un tooltip au survol
    - Cat_# : Ajoute une catégorie (utile si plusieurs applications avec chacune plusieurs services par exemple
    - Conf_# : Chemin complet vers le fichier de configuration, le cas échéant -> ATTENTION, NON FONCTIONNEL SUR SERVEUR DISTANT 
    - Log_# : Chemin complet vers le (dernier) log du service
    -->

    <add key="Service1" value="SrvCertificat_CSR;Service certificat;localhost"/>
    <add key="Tooltip_1" value="Service de création des CSR/P12"/>
    <add key="Cat_1" value="AMC unique"/>
    <add key="Conf_1" value="E:\AMC\Services\Certificat_CSR\AMC_UNIQUE.ServiceCertificat.exe.config"/>
    <add key="Log_1" value="E:\AMC\Services\Log\SrvCertificat_CSR00.txt"/>
    <add key="Service2" value="SrvMessagerieETFR_Aximum;Messagerie Aximum;localhost"/>
    <add key="Cat_2" value="AMC unique"/>
    <add key="Conf_2" value="E:\AMC\Services\Certificat_CSR\AMC_UNIQUE.ServiceCertificat.exe.config"/>
    <add key="Service3" value="SrvMessagerie;Service bidon;plop"/>
    <add key="Service4" value="SrvMessagerieETFR_Fareco;Messagerie Fareco;localhost"/>
    <add key="Service5" value="SrvMessagerieETFR_Fareco;Messagerie Fareco;10.10.10.9"/>
    <add  key="debug" value="1"/>
    <!-- Si des valeurs supplémentaires (donc optionnelles) sont ajoutés, il faut les définir ici -->
    <add key="valeursOptions" value=""/>
  </appSettings>
</configuration>